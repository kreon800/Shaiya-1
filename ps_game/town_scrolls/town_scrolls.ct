//hooks CUser::PacketPC
packet_hook:
movzx eax,word ptr[ebp+00]
cmp eax,0x055A //opcode
je packet
add eax,-0x501
jmp 004784DB

packet:
//save packet in memory
mov dl,byte ptr[ebp+02]
mov byte ptr[ecx+5930],dl //bag
mov dl,byte ptr[ebp+03]
mov byte ptr[ecx+5934],dl //slot
mov dl,byte ptr[ebp+04]
mov byte ptr[ecx+5938],dl //location

mov edi,ecx //user = ecx
jmp 00478F8D //case 0x050A

//hooks CUser::UpdateResetPosition
update_hook:
cmp eax,07
je consume
cmp eax,01
jne 0049DEB5
jmp 0049DDC8

consume:
mov eax,[edi+5934]
mov edx,[edi+5930]
push 00
push eax //slot
push edx //bag
mov ecx,edi //user = edi
call 004728E0 //CUser::ItemUseNSend
jmp 0049DDC8

//hooks CUser::ItemUse
effect_hook:
movzx ecx,byte ptr[eax+46]
cmp ecx,68 //effect 104
je itemid
cmp ecx,4A
ja 0047468A
jmp 00473170

teleport:
mov ecx,[esp+18]
add ecx,1388
mov [ebp+58B4],07 //type
mov [ebp+58B8],ecx //delay

//send view packet
mov eax,[ebp+DC] //charid
mov edx,0x0221 //opcode
push 06 //length
lea ecx,[esp+20]
mov word ptr[esp+20],dx
mov dword ptr[esp+22],eax
push ecx //packet
xor edx,edx
mov eax,ebp //user = ebp
call 004D56D0 //PSendViewCombat
jmp 0047469F

